force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu kover/Makefile
	cd $(top_srcdir) && perl admin/am_edit kover/Makefile.in


#>+ 10
kover_meta_unload.cpp:  cddb_fill.moc.cpp KoverTop.moc.cpp CDView.moc.cpp koverfile.moc.cpp PreferencesDialog.moc.cpp imagedlg.moc.cpp
	@echo 'creating kover_meta_unload.cpp'
	-rm -f kover_meta_unload.cpp
	@echo 'static const char * _metalist_kover[] = {' > kover_meta_unload.cpp
	cat  cddb_fill.moc.cpp KoverTop.moc.cpp CDView.moc.cpp koverfile.moc.cpp PreferencesDialog.moc.cpp imagedlg.moc.cpp | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> kover_meta_unload.cpp
	@echo '0};' >> kover_meta_unload.cpp
	@echo '#include <kunload.h>' >> kover_meta_unload.cpp
	@echo '_UNLOAD(kover)' >> kover_meta_unload.cpp


#>+ 3
clean-moc-classes:
	-rm -f  kover_meta_unload.cpp

#>+ 11
kover.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/imagedlgdata.cpp $(srcdir)/imagedlg.cpp $(srcdir)/cddb_fill.cpp $(srcdir)/koverfile.cpp $(srcdir)/KoverTop.cpp $(srcdir)/CDView.cpp $(srcdir)/main.cpp $(srcdir)/PreferencesDialog.cpp $(srcdir)/CDDB.cpp kover_meta_unload.cpp  KoverTop.moc.cpp cddb_fill.moc.cpp PreferencesDialog.moc.cpp koverfile.moc.cpp CDView.moc.cpp imagedlg.moc.cpp
	@echo 'creating kover.all_cpp.cpp ...'; \
	rm -f kover.all_cpp.files kover.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> kover.all_cpp.final; \
	for file in imagedlgdata.cpp imagedlg.cpp cddb_fill.cpp koverfile.cpp KoverTop.cpp CDView.cpp main.cpp PreferencesDialog.cpp CDDB.cpp kover_meta_unload.cpp  KoverTop.moc.cpp cddb_fill.moc.cpp PreferencesDialog.moc.cpp koverfile.moc.cpp CDView.moc.cpp imagedlg.moc.cpp; do \
	  echo "#include \"$$file\"" >> kover.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kover.all_cpp.final; \
	done; \
	cat kover.all_cpp.final kover.all_cpp.files  > kover.all_cpp.cpp; \
	rm -f kover.all_cpp.final kover.all_cpp.files

#>+ 3
clean-final:
	-rm -f kover.all_cpp.cpp

