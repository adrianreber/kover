#>+ 16
install-kde-icons:
	$(mkinstalldirs) $(DESTDIR)$(kde_icondir)/hicolor/32x32/apps
	$(INSTALL_DATA) $(srcdir)/hi32-app-kover.png $(DESTDIR)$(kde_icondir)/hicolor/32x32/apps/kover.png
	$(mkinstalldirs) $(DESTDIR)$(kde_icondir)/hicolor/48x48/apps
	$(INSTALL_DATA) $(srcdir)/hi48-app-kover.png $(DESTDIR)$(kde_icondir)/hicolor/48x48/apps/kover.png
	$(mkinstalldirs) $(DESTDIR)$(kde_icondir)/locolor/16x16/apps
	$(INSTALL_DATA) $(srcdir)/lo16-app-kover.png $(DESTDIR)$(kde_icondir)/locolor/16x16/apps/kover.png
	$(mkinstalldirs) $(DESTDIR)$(kde_icondir)/locolor/32x32/apps
	$(INSTALL_DATA) $(srcdir)/lo32-app-kover.png $(DESTDIR)$(kde_icondir)/locolor/32x32/apps/kover.png

uninstall-kde-icons:
	-rm -f $(DESTDIR)$(kde_icondir)/hicolor/32x32/apps/kover.png
	-rm -f $(DESTDIR)$(kde_icondir)/hicolor/48x48/apps/kover.png
	-rm -f $(DESTDIR)$(kde_icondir)/locolor/16x16/apps/kover.png
	-rm -f $(DESTDIR)$(kde_icondir)/locolor/32x32/apps/kover.png

